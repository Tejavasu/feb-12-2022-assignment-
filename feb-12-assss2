#compare the number with  equal or not
a=int(input(" enter the a value"))
b=int(input(" enter the b value"))
if a==b:
      print(" a is equal  to b")
elif a!=b:     
      print("a is not equal  to b")
elif a>b:
      print(" a is greater than b")
else:
      if a<b:
         print(" a is smaller than b")
      
